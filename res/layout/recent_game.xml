<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Push.archive.RegistrationResultActivity" >

    <ImageView
        android:id="@+id/championIcon"
        android:layout_width="32dip"
        android:layout_height="32dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:contentDescription="@string/champ_icon_label"
        android:src="@drawable/ahri" />

    <ImageView
        android:id="@+id/item5"
        android:layout_width="32dip"
        android:layout_height="32dip"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:contentDescription="@string/champ_icon_label"
        android:src="@drawable/ahri" />

    <ImageView
        android:id="@+id/item4"
        android:layout_width="32dip"
        android:layout_height="32dip"
        android:layout_toLeftOf="@+id/item5"
        android:contentDescription="@string/champ_icon_label"
        android:src="@drawable/ahri" />

    <ImageView
        android:id="@+id/item3"
        android:layout_width="32dip"
        android:layout_height="32dip"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/item4"
        android:contentDescription="@string/champ_icon_label"
        android:src="@drawable/ahri" />

    <ImageView
        android:id="@+id/item2"
        android:layout_width="32dip"
        android:layout_height="32dip"
        android:layout_toLeftOf="@+id/item3"
        android:contentDescription="@string/champ_icon_label"
        android:src="@drawable/ahri" />

    <ImageView
        android:id="@+id/item1"
        android:layout_width="32dip"
        android:layout_height="32dip"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/item2"
        android:contentDescription="@string/champ_icon_label"
        android:src="@drawable/ahri" />

    <ImageView
        android:id="@+id/item0"
        android:layout_width="32dip"
        android:layout_height="32dip"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/item1"
        android:contentDescription="@string/champ_icon_label"
        android:src="@drawable/ahri" />

    <TextView
        android:id="@+id/score"
        android:layout_width="wrap_content"
        android:layout_height="32dip"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/item0"
        android:layout_toRightOf="@+id/championIcon"
        android:text="TextView" />

</RelativeLayout>